//Fluxograma 

import javax.swing.JOptionPane;
    public class Exercicios1 {
    public static void main(String[] args) {          
            int salario;
            salario = Integer.parseInt(JOptionPane.showInputDialog("Digite sua Idade"));
            if(salario >= 1212){
                JOptionPane.showMessageDialog(null, "Você está ganhando pelo menos um salario mínimo");
            }                      
        }
    }
    
